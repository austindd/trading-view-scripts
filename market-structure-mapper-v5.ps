// ============================================================================
// HELPER FUNCTIONS
// ============================================================================

// Check if current candle is bearish and closes below previous candle's low
isBearishBreakdown() =>
    close < open and close < low[1]

// Check if current candle is bullish and closes above previous candle's high
isBullishBreakout() =>
    close > open and close > high[1]

// Find lowest low between two bar indices
findLowestBetween(startBar, endBar) =>
    float lowestPrice = na
    int lowestBarIndex = na

    int start = startBar
    int end = endBar

    if start > end
        int temp = start
        start := end
        end := temp

    int currentBar = bar_index
    int lookbackStart = currentBar - end
    int lookbackEnd = currentBar - start - 1

    lookbackStart := math.max(lookbackStart, 0)
    lookbackEnd := math.min(lookbackEnd, 5000)

    if lookbackStart <= lookbackEnd
        for i = lookbackStart to lookbackEnd
            if not na(low[i])
                if na(lowestPrice) or low[i] < lowestPrice
                    lowestPrice := low[i]
                    lowestBarIndex := currentBar - i

    [lowestPrice, lowestBarIndex]

// Find highest high between two bar indices
findHighestBetween(startBar, endBar) =>
    float highestPrice = na
    int highestBarIndex = na

    int start = startBar
    int end = endBar

    if start > end
        int temp = start
        start := end
        end := temp

    int currentBar = bar_index
    int lookbackStart = currentBar - end
    int lookbackEnd = currentBar - start - 1

    lookbackStart := math.max(lookbackStart, 0)
    lookbackEnd := math.min(lookbackEnd, 5000)

    if lookbackStart <= lookbackEnd
        for i = lookbackStart to lookbackEnd
            if not na(high[i])
                if na(highestPrice) or high[i] > highestPrice
                    highestPrice := high[i]
                    highestBarIndex := currentBar - i

    [highestPrice, highestBarIndex]

// ============================================================================
// ZONE CALCULATION FUNCTIONS
// ============================================================================

// Calculate demand zone from a valid low
// Returns [zoneTop, zoneBottom, leftBarIndex] or [na, na, na] if zone cannot be calculated
calculateZoneFromLow(int lowBarIndex) =>
    float zoneTop = na
    float zoneBottom = na
    int leftBarIndex = lowBarIndex

    // Find all bearish candles to the left of the low until we hit a bullish candle
    int barsBack = bar_index - lowBarIndex

    if barsBack >= 0 and barsBack < 5000
        float highestInZone = na
        float lowestInZone = na
        int leftmostBarIndex = lowBarIndex
        bool foundFirstBearish = false

        // Scan left starting from one bar before the low bar
        // Continue until we find at least one bearish candle
        for i = barsBack + 1 to math.min(barsBack + 500, 5000)
            if not na(open[i]) and not na(close[i])
                bool isBearish = close[i] < open[i]

                if isBearish
                    // Found a bearish candle - include it in the zone
                    foundFirstBearish := true
                    leftmostBarIndex := bar_index - i
                    if na(highestInZone) or high[i] > highestInZone
                        highestInZone := high[i]
                    if na(lowestInZone) or low[i] < lowestInZone
                        lowestInZone := low[i]
                else if foundFirstBearish
                    // We already found bearish candles, and now hit a bullish one - stop
                    break
                // else: haven't found a bearish candle yet, keep scanning left

        // If we found at least one bearish candle, use that for the zone
        if not na(highestInZone) and not na(lowestInZone)
            zoneTop := highestInZone
            zoneBottom := lowestInZone
            leftBarIndex := leftmostBarIndex
        // Fallback: if no bearish candles found to the left, use the low bar itself
        else
            // Use a small zone around the low point
            float lowValue = barsBack >= 0 and not na(low[barsBack]) ? low[barsBack] : na
            float highValue = barsBack >= 0 and not na(high[barsBack]) ? high[barsBack] : na
            if not na(lowValue) and not na(highValue)
                zoneTop := highValue
                zoneBottom := lowValue
                leftBarIndex := lowBarIndex

    [zoneTop, zoneBottom, leftBarIndex]

// Calculate supply zone from a valid high
// Returns [zoneTop, zoneBottom, leftBarIndex] or [na, na, na] if zone cannot be calculated
calculateZoneFromHigh(int highBarIndex) =>
    float zoneTop = na
    float zoneBottom = na
    int leftBarIndex = highBarIndex

    // Find all bullish candles to the left of the high until we hit a bearish candle
    int barsBack = bar_index - highBarIndex

    if barsBack >= 0 and barsBack < 5000
        float highestInZone = na
        float lowestInZone = na
        int leftmostBarIndex = highBarIndex
        bool foundFirstBullish = false

        // Scan left starting from one bar before the high bar
        // Continue until we find at least one bullish candle
        for i = barsBack + 1 to math.min(barsBack + 500, 5000)
            if not na(open[i]) and not na(close[i])
                bool isBullish = close[i] > open[i]

                if isBullish
                    // Found a bullish candle - include it in the zone
                    foundFirstBullish := true
                    leftmostBarIndex := bar_index - i
                    if na(highestInZone) or high[i] > highestInZone
                        highestInZone := high[i]
                    if na(lowestInZone) or low[i] < lowestInZone
                        lowestInZone := low[i]
                else if foundFirstBullish
                    // We already found bullish candles, and now hit a bearish one - stop
                    break
                // else: haven't found a bullish candle yet, keep scanning left

        // If we found at least one bullish candle, use that for the zone
        if not na(highestInZone) and not na(lowestInZone)
            zoneTop := highestInZone
            zoneBottom := lowestInZone
            leftBarIndex := leftmostBarIndex
        // Fallback: if no bullish candles found to the left, use the high bar itself
        else
            // Use a small zone around the high point
            float lowValue = barsBack >= 0 and not na(low[barsBack]) ? low[barsBack] : na
            float highValue = barsBack >= 0 and not na(high[barsBack]) ? high[barsBack] : na
            if not na(lowValue) and not na(highValue)
                zoneTop := highValue
                zoneBottom := lowValue
                leftBarIndex := highBarIndex

    [zoneTop, zoneBottom, leftBarIndex]

// Check if a zone should be aligned (for CTF zones in aligned mode)
// A CTF zone is aligned if it corresponds to a HTF high or low
isZoneAlignedWithHtf(int levelBarIndex, bool isLow) =>
    bool aligned = false

    // Check if this CTF level bar matches any HTF level bar (within tolerance)
    if isLow
        if array.size(htfLows) > 0
            for i = 0 to array.size(htfLows) - 1
                htfLow = array.get(htfLows, i)
                // Check if bars are close enough (within a few bars tolerance)
                if math.abs(levelBarIndex - htfLow.barIndex) < 10
                    aligned := true
                    break
    else
        if array.size(htfHighs) > 0
            for i = 0 to array.size(htfHighs) - 1
                htfHigh = array.get(htfHighs, i)
                if math.abs(levelBarIndex - htfHigh.barIndex) < 10
                    aligned := true
                    break

    aligned

// ============================================================================
// LEVEL MANAGEMENT FUNCTIONS
// ============================================================================

// Add a new high level
addHigh(float price, int barIdx, string levelType, color lineColor) =>
    // Delete previous most recent high line if it's not broken or swept
    if mostRecentHighIndex >= 0
        prevHigh = array.get(highs, mostRecentHighIndex)
        if prevHigh.levelType != "broken" and prevHigh.levelType != "swept"
            line.delete(prevHigh.levelLine)

    // Create new line
    newLine = line.new(barIdx, price, barIdx + 1, price,
                       color=lineColor,
                       width=lineWidth,
                       extend=extend.right,
                       style=line.style_solid)

    // Create new HighLevel object
    newHigh = HighLevel.new(price, barIdx, newLine, levelType)
    array.push(highs, newHigh)

    // Create label if enabled
    if showHighLowLabels
        label.new(barIdx, price, "",
                 style=label.style_label_down,
                 color=lineColor,
                 textcolor=color.white,
                 size=size.tiny)

    true

// Add a new low level
addLow(float price, int barIdx, string levelType, color lineColor) =>
    // Delete previous most recent low line if it's not broken or swept
    if mostRecentLowIndex >= 0
        prevLow = array.get(lows, mostRecentLowIndex)
        if prevLow.levelType != "broken" and prevLow.levelType != "swept"
            line.delete(prevLow.levelLine)

    // Create new line
    newLine = line.new(barIdx, price, barIdx + 1, price,
                       color=lineColor,
                       width=lineWidth,
                       extend=extend.right,
                       style=line.style_solid)

    // Create new LowLevel object
    newLow = LowLevel.new(price, barIdx, newLine, levelType)
    array.push(lows, newLow)

    // Create label if enabled
    if showHighLowLabels
        label.new(barIdx, price, "",
                 style=label.style_label_up,
                 color=lineColor,
                 textcolor=color.white,
                 size=size.tiny)

    true

// HTF versions
addHighHTF(float price, int barIdx, string levelType, color lineColor) =>
    if htfMostRecentHighIndex >= 0
        prevHigh = array.get(htfHighs, htfMostRecentHighIndex)
        if prevHigh.levelType != "broken" and prevHigh.levelType != "swept"
            line.delete(prevHigh.levelLine)

    newLine = line.new(barIdx, price, barIdx + 1, price,
                       color=lineColor,
                       width=lineWidth + 1,  // Thicker for HTF
                       extend=extend.right,
                       style=line.style_solid)

    newHigh = HighLevel.new(price, barIdx, newLine, levelType)
    array.push(htfHighs, newHigh)

    if showHighLowLabels
        label.new(barIdx, price, "",
                 style=label.style_label_down,
                 color=lineColor,
                 textcolor=color.white,
                 size=size.small)

    true

