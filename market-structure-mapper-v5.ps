// ============================================================================
// TYPE DEFINITIONS
// ============================================================================

// Type for tracking high levels
type HighLevel
    float price
    int barIndex
    line levelLine
    string levelType  // "strong", "weak", "broken", "swept"

// Type for tracking low levels
type LowLevel
    float price
    int barIndex
    line levelLine
    string levelType  // "strong", "weak", "broken", "swept"

// Type for tracking supply/demand zones
type Zone
    float top
    float bottom
    int barIndex
    box zoneBox
    bool broken
    bool isHtfAligned  // Track if this CTF zone aligns with HTF zone

